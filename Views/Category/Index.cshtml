@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="d-lg-table table-bordered table-striped"style=100%>
    <thead>
        <tr>
            <th>
                categpry name 
            </th>
            <th>
                display order 
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>

